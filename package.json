{
  "name": "blu-react-desktop",
  "version": "1.2.1",
  "description": "A React based version of the BLU desktop app.",
  "repository": {
    "type": "git",
    "url": "https://github.com/katima-g33k/blu-react-desktop"
  },
  "author": "Jessy Lachapelle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katima-g33k/blu-react-desktop/issues"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "electron": "2.0.7",
    "electron-builder": "20.27.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "blu-api": "https://github.com/katima-g33k/blu-api.git#v2.0.0",
    "crypto": "0.0.3",
    "file-saver": "1.3.8",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-bootstrap": "0.32.1",
    "react-bootstrap-table": "4.3.1",
    "react-datepicker": "1.6.0",
    "react-dom": "16.4.2",
    "react-i18nify": "1.11.14",
    "react-loading": "2.0.3",
    "react-redux": "5.0.7",
    "react-router": "3.2.1",
    "recharts": "1.1.0",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "request": "2.87.0"
  },
  "main": "./electron.js",
  "homepage": "./",
  "scripts": {
    "build": "react-scripts build",
    "dist": "npm run build && build",
    "dist:win": "npm run build && build --win --x64",
    "electron": "electron -r babel-register .",
    "lint": "eslint ./src",
    "pack": "build --dir",
    "start": "react-scripts start"
  },
  "build": {
    "extends": null,
    "appId": "com.blu.react-desktop",
    "productName": "BLU",
    "directories": {
      "buildResources": "public"
    }
  }
}
