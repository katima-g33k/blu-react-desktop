{
  "name": "blu-react-desktop",
  "version": "0.1.0",
  "description": "A React based version of the BLU desktop app.",
  "repository": {
    "type": "git",
    "url": "https://github.com/katima-g33k/blu-react-desktop"
  },
  "author": "Jessy Lachapelle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katima-g33k/blu-react-desktop/issues"
  },
  "devDependencies": {
    "electron": "^1.7.3",
    "electron-builder": "^18.6.2",
    "eslint": "^3.9.1",
    "react-scripts": "0.8.4"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "halogen": "^0.2.0",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.6",
    "react-bootstrap-table": "3.0.0-beta.11",
    "react-dom": "^15.4.1",
    "react-i18nify": "^1.6.3",
    "react-router": "^3.0.0",
    "request": "^2.79.0"
  },
  "main": "./electron.js",
  "homepage": "./",
  "scripts": {
    "build": "react-scripts build",
    "dist": "yarn run build && build",
    "dist:win": "yarn run build && build --win --x64",
    "electron": "electron -r babel-register .",
    "pack": "build --dir",
    "start": "react-scripts start"
  },
  "build": {
    "appId": "com.blu.react-desktop",
    "productName": "BLU",
    "directories": {
      "buildResources": "public"
    }
  }
}
