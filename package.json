{
  "name": "blu-react-desktop",
  "version": "1.1.2",
  "description": "A React based version of the BLU desktop app.",
  "repository": {
    "type": "git",
    "url": "https://github.com/katima-g33k/blu-react-desktop"
  },
  "author": "Jessy Lachapelle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katima-g33k/blu-react-desktop/issues"
  },
  "devDependencies": {
    "electron": "^1.7.3",
    "electron-builder": "^18.6.2",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^7.4.0",
    "react-scripts": "0.8.4"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "blu-api": "https://github.com/katima-g33k/blu-api.git#v2.0.0",
    "crypto": "^0.0.3",
    "file-saver": "^1.3.3",
    "halogen": "^0.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "prop-types": "^15.6.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.6",
    "react-bootstrap-table": "3.0.0-beta.11",
    "react-datepicker": "^0.52.0",
    "react-dom": "^15.4.1",
    "react-i18nify": "^1.6.3",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.0",
    "recharts": "^1.0.0-alpha.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0"
  },
  "main": "./electron.js",
  "homepage": "./",
  "scripts": {
    "build": "react-scripts build",
    "dist": "yarn run build && build",
    "dist:win": "yarn run build && build --win --x64",
    "electron": "electron -r babel-register .",
    "lint": "eslint ./src",
    "pack": "build --dir",
    "start": "react-scripts start"
  },
  "build": {
    "appId": "com.blu.react-desktop",
    "productName": "BLU",
    "directories": {
      "buildResources": "public"
    }
  }
}
